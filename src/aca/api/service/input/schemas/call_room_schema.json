{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "room": {
      "type": ["integer"],
      "minimum": 0,
      "maximum": 255
    },
    "call_status": {
      "type": "string",
      "enum": ["ANSWERED", "NOT_ANSWERED", "INACTIVE"]
    }
  },
  "required": ["room", "call_status"]
}